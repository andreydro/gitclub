<%= render partial: 'layouts/repo_heading' %>
<div class="container">
  <div class="subnav" data-pjax>
    <a href="#" class="btn btn-primary float-right" data-hotkey="c" tabindex="0" data-ga-click="Repository, go to compare view, location: issue list; New issue">
      New issue
    </a>
    <div class="float-left subnav-spacer-right" role="search">
      <div class="float-left">
        <button class="btn select-menu-button btn-secondary dropdown-toggle" type="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false" >
          Filters
        </button>
        <div class="dropdown-menu" aria-expanded="false">
          <div class="select-menu-model">
            <div class="select-menu-list">
              <a href="#" class="select-menu-item ">
                <div class="select-menu-item-text">
                  Open issues and pull requests
                </div>
              </a>
              <a href="#" class="select-menu-item">
                <div class="select-menu-item-text">
                  Your issues
                </div>
              </a>
              <a href="#" class="select-menu-item">
                <div class="select-menu-item-text">
                  Your pull requests
                </div>
              </a>
              <a href="#" class="select-menu-item">
                <div class="select-menu-item-text">
                  Everything assigned to you
                </div>
              </a>
              <a href="#" class="select-menu-item">
                <div class="select-menu-item-text">
                  Everything mentioning you
                </div>
              </a>
              <a href="#" class="select-menu-item">
                <div class="select-menu-item-text">
                  View advanced search syntax
                </div>
              </a>
            </div>
          </div>
        </div>
      </div>
      <form accept-charset="UTF-8" action="#" class="subnav-search float-left" data-pjax="true" method="get">
        <div style="margin: 0; padding: 0; display: inline;">
          <input name="utf8" type="hidden" value="+">
        </div>
        <input aria-label="Search all issues" class="form-control subnav-search-input input-contrast" data-hotkey="/" id="js-issues-search"
               name="q" placeholder="Search all issues" type="text" value="is:issue is:open">
        <svg aria-hidden="true" class="octicon octicon-search subnav-search-icon" height="16" version="1.1" viewBox="0 0 16 16" width="16">
